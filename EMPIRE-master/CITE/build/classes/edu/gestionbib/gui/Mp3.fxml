<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.gestionbib.gui.Mp3Controller">
   <children>
      <Button fx:id="lecture" layoutX="223.0" layoutY="301.0" mnemonicParsing="false" onAction="#lecture" prefHeight="40.0" prefWidth="68.0" text="LECTURE">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
      <Button fx:id="pause" layoutX="315.0" layoutY="301.0" mnemonicParsing="false" onAction="#pause" prefHeight="40.0" prefWidth="68.0" text="PAUSE">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
      <Slider fx:id="volume" layoutX="526.0" layoutY="130.0" onDragDetected="#click" orientation="VERTICAL" />
      <MediaView fx:id="mediaview" fitHeight="200.0" fitWidth="200.0" layoutX="238.0" layoutY="25.0" />
      <AnchorPane prefHeight="400.0" prefWidth="206.0" style="-fx-background-color: #2F4F4F;">
         <children>
            <Button fx:id="accueil" layoutX="26.0" layoutY="349.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="141.0" text="ACCUEIL">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>

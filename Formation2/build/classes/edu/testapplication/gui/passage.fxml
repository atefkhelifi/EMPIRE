<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.testapplication.gui.PassageController">
   <children>
      <Button fx:id="affiche" layoutX="29.0" layoutY="257.0" mnemonicParsing="false" onAction="#affiche" text="afficher formation" />
      <Button fx:id="inscire" layoutX="29.0" layoutY="313.0" mnemonicParsing="false" onAction="#inscrire" prefHeight="31.0" prefWidth="139.0" text="s'inscrire" />
      <TableView fx:id="table_formation_user" prefHeight="200.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="id_formation" prefWidth="75.0" text="id_formation" />
          <TableColumn fx:id="nom_formation" prefWidth="75.0" text="nom_formation" />
            <TableColumn fx:id="sujet_formation" prefWidth="75.0" text="sujet_formation" />
            <TableColumn fx:id="nom_formateur" prefWidth="75.0" text="nom_formateur" />
            <TableColumn fx:id="nombre_place_formation" prefWidth="75.0" text="nombre_place_formation" />
            <TableColumn fx:id="date_formation" prefWidth="75.0" text="date_formation" />
            <TableColumn fx:id="localisation" prefWidth="75.0" text="localisation" />
        </columns>
      </TableView>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="150.0" fitWidth="260.0" layoutX="240.0" layoutY="226.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../image/bouton-acces-client.jpg" />
         </image>
      </ImageView>
      <Button fx:id="acceuil" layoutX="29.0" layoutY="211.0" mnemonicParsing="false" onAction="#acceuil" prefHeight="31.0" prefWidth="139.0" text="Acceuil" />
   </children>
</AnchorPane>
